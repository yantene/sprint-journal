{
	"name": "sprint-journal",
	"version": "0.1.0",
	"description": "Provides an ISO week-based journaling methodology and templates for daily and task journals.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"lint": "biome check .",
		"lint:fix": "biome check --write ."
	},
	"keywords": [
		"obsidian",
		"plugin",
		"journal",
		"sprint",
		"iso-week"
	],
	"author": "Shuhei YOSHIDA",
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "2.3.4",
		"@types/node": "^16.11.6",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	}
}
